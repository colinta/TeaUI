{
  "name": "wretched",
  "description": "A low-level terminal interface library for node.js.",
  "author": "Blessed by Christopher Jeffrey, Wretched by Colin T.A. Gray",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "preferGlobal": false,
  "repository": "git://github.com/colinta/wretched.git",
  "homepage": "https://github.com/colinta/wretched",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "http://github.com/colinta/wretched/issues"
  },
  "scripts": {
    "clean": "rm -f dist/**/*.js",
    "cp-usr": "mkdir -p dist/sys/ && cp -r lib/sys/usr dist/sys/",
    "demo": "yarn clean && yarn cp-usr && tsc && cd dist/demos/ && node --enable-source-maps --"
  },
  "keywords": [
    "curses",
    "tui",
    "terminal",
    "terminal-ui"
  ],
  "tags": [
    "curses",
    "tui",
    "terminal"
  ],
  "engines": {
    "node": ">= 20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "typescript": "^5.1.6"
  }
}
