{
  "name": "wretched",
  "description": "A low-level terminal interface library for node.js.",
  "author": "Colin T.A. Gray <colinta@colinta.com>",
  "contributors": [
    "Shawn Chen <meishangchen@gmail.com>",
    "Christopher Jeffrey (via Blessed)"
  ],
  "version": "1.0.1-alpha",
  "license": "MIT",
  "preferGlobal": false,
  "repository": "git://github.com/wretched-tui/wretched.git",
  "homepage": "https://github.com/wretched-tui/wretched",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "bugs": {
    "url": "http://github.com/wretched-tui/wretched/issues"
  },
  "scripts": {
    "publish": "npm publish --tag alpha",
    "prepublishOnly": "yarn clean && yarn tsc --declaration dist/index.d.ts && yarn cp-usr && rm -rf dist/demos/",
    "clean": "rm -rf dist/",
    "cp-usr": "mkdir -p dist/sys/usr/ && cp lib/sys/usr/* dist/sys/usr/",
    "build": "yarn clean && yarn tsc && yarn cp-usr",
    "demo": "yarn build && cd dist/demos/ && node --enable-source-maps --"
  },
  "keywords": [
    "curses",
    "tui",
    "terminal",
    "terminal-ui"
  ],
  "tags": [
    "curses",
    "tui",
    "terminal"
  ],
  "engines": {
    "node": ">= 18.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "typescript": "^5.1.6"
  }
}
